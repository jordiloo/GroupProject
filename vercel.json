{
    "version": 2,
    "builds": [
      {
        "src": "server/server.js",
        "use": "@vercel/node",
        "config": {
          "maxDuration": 10
        }
      },
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }