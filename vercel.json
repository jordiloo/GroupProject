{
    "version": 2,
    "builds": [
      {
        "src": "server/server.js",
        "use": "@vercel/node",
        "config": {
          "maxDuration": 10
        }
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }